        dct:abstract     "Apesar de não ser algo muito comum, crianças e bebês de todas as idades podem ter pedra na vesícula." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:jntc ;
        dct:identifier   "6773555210416773586"^^<http://www.w3.org/2001/XMLSchema#long> ;
