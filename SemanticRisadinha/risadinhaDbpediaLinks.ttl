        dct:abstract     "Na maioria dos casos de comunicação interatrial, nenhum tratamento é necessário. Alguns casos podem necessitar de remédios ou cirurgia." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:icsp ;
        dct:identifier   "3661562646860960807"^^<http://www.w3.org/2001/XMLSchema#long> ;
