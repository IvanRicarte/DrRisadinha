        a                sioc:Post ;
        dct:abstract     "O tratamento da síndrome dos ovários policísticos é feito com medicamentos e associação de dieta e atividade física." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:ccd ;
        dct:identifier   "2843045971491618143"^^<http://www.w3.org/2001/XMLSchema#long> ;
