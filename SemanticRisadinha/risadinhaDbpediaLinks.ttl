        a                sioc:Post ;
        dct:abstract     "Fissura anal é um machucado no canal anal, geralmente causada pela passagem de fezes ressecadas ou muito volumosas." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:lms ;
        dct:identifier   "5130977323258539005"^^<http://www.w3.org/2001/XMLSchema#long> ;
        dct:isPartOf     <http://www.drrisadinha.org.br/> ;
        dct:issued       "2017-05-31T09:00" ;
