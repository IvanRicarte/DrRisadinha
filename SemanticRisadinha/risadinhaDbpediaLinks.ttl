        a                sioc:Post ;
        dct:abstract     "O testículo pode descer espontaneamente até os 6 meses de idade. Quando isto não ocorre, uma cirurgia pode ser necessária." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:ccd ;
