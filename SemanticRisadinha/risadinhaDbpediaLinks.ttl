        dct:abstract     "O tratamento da candidíase oral é feito com medidas de higiene e uso de medicamentos antifúngicos." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:nss ;
        dct:identifier   "796044445058946553"^^<http://www.w3.org/2001/XMLSchema#long> ;
