        a                sioc:Post ;
        dct:abstract     "O tratamento da vasculite, a depender do caso, é realizado com medicamentos corticoides, medicamentos imunossupressores ou medicamentos biológicos." ;
        dct:contributor  doc:gnc , doc:mcbg ;
        dct:creator      doc:nss ;
