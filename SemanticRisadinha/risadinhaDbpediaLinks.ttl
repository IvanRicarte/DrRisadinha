        a                sioc:Post ;
        dct:abstract     "O tratamento para a síndrome de Sotos é sintomático, ou seja, busca aliviar os sintomas. A maioria dos sintomas desaparece na puberdade." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:cvar ;
        dct:identifier   "4304993680226245099"^^<http://www.w3.org/2001/XMLSchema#long> ;
