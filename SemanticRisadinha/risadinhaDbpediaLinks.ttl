        dct:abstract     "No tratamento da crupe, podem ser usados medicamentos, inalação, oxigênio e intubação." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:nss ;
        dct:identifier   "7847043252056259195"^^<http://www.w3.org/2001/XMLSchema#long> ;
