        dct:abstract     "A trombose da veia porta é o entupimento dessa veia, causando dor abdominal, dor nas costas, enjoo, febre, diarreia sem sangue ou aumento da barriga." ;
        dct:contributor  doc:mcap , doc:fc , doc:mcbg ;
        dct:creator      doc:icat ;
        dct:identifier   "6826970945199941975"^^<http://www.w3.org/2001/XMLSchema#long> ;
