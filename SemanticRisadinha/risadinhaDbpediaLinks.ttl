        a                sioc:Post ;
        dct:abstract     "A urticária é uma irritação na pele caracterizada pelo surgimento de manchas vermelhas e inchadas na pele, e muita coceira." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:gjmc ;
        dct:identifier   "1607976291075148545"^^<http://www.w3.org/2001/XMLSchema#long> ;
