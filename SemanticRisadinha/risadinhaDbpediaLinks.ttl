        dct:abstract     "O tratamento da displasia broncopulmonar é feito com oxigênio, medicamentos e uma dieta rica em calorias e vitaminas A e E." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:icsp ;
        dct:identifier   "8068947617999988419"^^<http://www.w3.org/2001/XMLSchema#long> ;
