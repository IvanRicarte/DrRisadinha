        a                sioc:Post ;
        dct:abstract     "A síndrome nefrótica, geralmente, é tratada com medicamentos anti-inflamatórios e imunossupressores, e desaparece na adolescência." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:cvar ;
        dct:identifier   "5850716797543811453"^^<http://www.w3.org/2001/XMLSchema#long> ;
