        dct:abstract     "O tratamento de malformação de Chiari depende da forma, gravidade e sintomas associados. Em alguns casos, a cirurgia pode ser necessária." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:cvar ;
        dct:identifier   "7762195093287723098"^^<http://www.w3.org/2001/XMLSchema#long> ;
