        a                sioc:Post ;
        dct:abstract     "O tratamento da sarna é feito com pomadas, cremes, loções ou comprimidos, além de medidas para desinfetar roupas, lençóis e toalhas." ;
        dct:contributor  doc:fc , doc:mcbg , doc:icsp ;
