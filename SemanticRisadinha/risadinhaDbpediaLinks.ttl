        a                sioc:Post ;
        dct:abstract     "Diabetes mellitus é uma doença caracterizada pelo aumento do açúcar no sangue." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:aaf ;
        dct:identifier   "915370186465615621"^^<http://www.w3.org/2001/XMLSchema#long> ;
