        a                sioc:Post ;
        dct:abstract     "A trombose da veia porta é tratada com medicamentos anticoagulantes, trombolíticos, e com cirurgias." ;
        dct:contributor  doc:mcap , doc:fc , doc:mcbg ;
        dct:creator      doc:icat ;
        dct:identifier   "7288486107731697946"^^<http://www.w3.org/2001/XMLSchema#long> ;
