        a                sioc:Post ;
        dct:abstract     "O tratamento da obesidade em crianças e adolescentes demanda mudanças alimentares e mudanças de comportamento, como aumento da atividade física." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:loa ;
        dct:identifier   "8038216599209507721"^^<http://www.w3.org/2001/XMLSchema#long> ;
