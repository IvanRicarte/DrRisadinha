        a                sioc:Post ;
        dct:abstract     "Não existe atualmente um tratamento que cure a paralisia cerebral, mas há diversas medidas que melhoram a qualidade de vida da criança." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:loa ;
        dct:identifier   "9015954191794023029"^^<http://www.w3.org/2001/XMLSchema#long> ;
