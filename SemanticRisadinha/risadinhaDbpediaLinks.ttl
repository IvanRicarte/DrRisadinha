        dct:abstract     "A drenagem anômala total das veias pulmonares é um defeito na conexão entre o coração e as veias que trazem sangue dos pulmões para o coração." ;
        dct:contributor  doc:fc , doc:mcbg , doc:icsp ;
        dct:creator      doc:nss ;
        dct:identifier   "6483906410897059672"^^<http://www.w3.org/2001/XMLSchema#long> ;
