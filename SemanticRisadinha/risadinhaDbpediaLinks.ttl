        a                sioc:Post ;
        dct:abstract     "O ressecamento da pele, os machucados, as rinites, as sinusites, as alergias, os desvios de septo, certas doenças e substâncias causam sangramento do nariz." ;
        dct:contributor  doc:gnc , doc:mcbg ;
        dct:creator      doc:jntc ;
        dct:identifier   "5352916378912717238"^^<http://www.w3.org/2001/XMLSchema#long> ;
