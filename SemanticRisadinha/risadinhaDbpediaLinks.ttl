        dct:abstract     "O tratamento da distrofia muscular de Duchenne tem como objetivo a redução dos sintomas e a melhora da qualidade de vida do paciente." ;
        dct:contributor  doc:mcap , doc:fc , doc:mcbg ;
        dct:creator      doc:nss ;
        dct:identifier   "2135352789618339895"^^<http://www.w3.org/2001/XMLSchema#long> ;
