        a                sioc:Post ;
        dct:abstract     "Esferocitose hereditária moderada e grave pode ser tratada com transfusões de sangue, medicamentos e cirurgia para remoção do baço." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:cvar ;
        dct:identifier   "2111026994745564179"^^<http://www.w3.org/2001/XMLSchema#long> ;
