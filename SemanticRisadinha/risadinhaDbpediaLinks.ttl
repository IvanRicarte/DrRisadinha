        a                sioc:Post ;
        dct:abstract     "A atrofia muscular espinhal, ou AME, é caracterizada por fraqueza e atrofia nos músculos das pernas e dos braços." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:cvar ;
        dct:identifier   "2033116216619351506"^^<http://www.w3.org/2001/XMLSchema#long> ;
