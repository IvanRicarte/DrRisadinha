        dct:abstract     "Meninge é a inflamação das meninges (membranas que recobrem o cérebro e a medula espinhal), causada principalmente por infecções." ;
        dct:contributor  doc:gnc , doc:mcbg ;
        dct:creator      doc:gjmc ;
        dct:identifier   "1337914043277482864"^^<http://www.w3.org/2001/XMLSchema#long> ;
