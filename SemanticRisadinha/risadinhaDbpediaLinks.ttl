        a                sioc:Post ;
        dct:abstract     "Remédios caseiros são eficazes para o tratamento de alguns sintomas mas, em caso de não melhora, um médico deve sempre ser consultado." ;
        dct:contributor  doc:mcbg ;
        dct:creator      doc:fc ;
        dct:identifier   "2398984796981454703"^^<http://www.w3.org/2001/XMLSchema#long> ;
