        dct:abstract     "Tireoidite aguda é uma doença inflamatória da tireoide, causada por infecção, radiação, medicamentos ou trauma." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:fps ;
        dct:identifier   "3604944432705152138"^^<http://www.w3.org/2001/XMLSchema#long> ;
