        dct:abstract     "O tratamento para fissura anal envolve alimentação rica em fibras, ingestão de grande quantidade de água, banhos de assento, medicamentos e até cirurgia." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:lms ;
        dct:identifier   "4612798981556717747"^^<http://www.w3.org/2001/XMLSchema#long> ;
