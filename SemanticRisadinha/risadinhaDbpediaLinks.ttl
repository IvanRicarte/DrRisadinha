        dct:abstract     "O tratamento da neurofibromatose tipo 1 depende dos sinais e sintomas, podendo ser indicados: cirurgia, uso de laser ou radiação, e medicamentos." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:icsp ;
        dct:identifier   "6558016179061695620"^^<http://www.w3.org/2001/XMLSchema#long> ;
