        a                sioc:Post ;
        dct:abstract     "O refluxo gastroesofágico é uma condição em que parte do alimento que está no estômago volta para o esôfago e às vezes até a boca, com ou sem vômito." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:ccd ;
