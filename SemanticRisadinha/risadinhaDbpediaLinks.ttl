        a                sioc:Post ;
        dct:abstract     "Na maioria dos casos de hematoma extradural se realiza uma cirurgia para retirar o sangue acumulado e reverter o aumento da pressão dentro do crânio." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:lms ;
        dct:identifier   "196070705128141622"^^<http://www.w3.org/2001/XMLSchema#long> ;
