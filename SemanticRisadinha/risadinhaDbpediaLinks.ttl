        a                sioc:Post ;
        dct:abstract     "O edema pulmonar é o excesso de líquido nos pulmões, causando falta de ar e inchaço em algumas partes do corpo." ;
        dct:contributor  doc:gnc , doc:mcbg ;
        dct:creator      doc:jntc ;
        dct:identifier   "5160529009914474218"^^<http://www.w3.org/2001/XMLSchema#long> ;
