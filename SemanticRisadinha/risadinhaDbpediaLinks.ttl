        dct:abstract     "A paralisia cerebral infantil é um conjunto de alterações do movimento e da postura da criança causadas por lesões cerebrais." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:loa ;
        dct:identifier   "578158691058001785"^^<http://www.w3.org/2001/XMLSchema#long> ;
