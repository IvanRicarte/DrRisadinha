        dct:abstract     "O tratamento da anemia por deficiência de ferro é feito com alimentos ricos em ferro e em vitamina C e com medicamentos contendo ferro." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:icsp ;
        dct:identifier   "7173755562738590395"^^<http://www.w3.org/2001/XMLSchema#long> ;
