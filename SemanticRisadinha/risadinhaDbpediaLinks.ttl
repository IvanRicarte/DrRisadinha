        dct:abstract     "O tratamento para o transtorno opositivo desafiador é baseado em psicoterapia e uso de medicamentos." ;
        dct:contributor  doc:fc , doc:maa , doc:mcbg ;
        dct:creator      doc:cvar ;
        dct:identifier   "5668270780094514879"^^<http://www.w3.org/2001/XMLSchema#long> ;
