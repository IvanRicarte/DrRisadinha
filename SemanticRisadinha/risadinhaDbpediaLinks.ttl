        dct:abstract     "Para muitos casos de escoliose não há a necessidade de tratamento. Quando necessário, o tratamento consiste em uso de colete ou cirurgia." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:lms ;
        dct:identifier   "4995369295847282754"^^<http://www.w3.org/2001/XMLSchema#long> ;
