        a                sioc:Post ;
        dct:abstract     "O refluxo vesicoureteral ocorre quando a urina pode voltar, da bexiga, para um ou ambos os ureteres e, em alguns casos, para um ou ambos os rins." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:cvar ;
        dct:identifier   "770678199115137473"^^<http://www.w3.org/2001/XMLSchema#long> ;
