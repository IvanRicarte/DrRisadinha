        dct:abstract     "A coqueluche é tratada com medicamentos, ingestão de água, respiração de ar limpo e alimentação adequada. Em casos graves, o paciente poderá ser internado." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:gjmc ;
        dct:identifier   "611352180426527835"^^<http://www.w3.org/2001/XMLSchema#long> ;
        dct:isPartOf     <http://www.drrisadinha.org.br/> ;
