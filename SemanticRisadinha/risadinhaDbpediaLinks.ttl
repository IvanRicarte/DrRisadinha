        a                sioc:Post ;
        dct:abstract     "Histerectomia é uma cirurgia para remover o útero, e é uma das formas de tratamento da endometriose." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:cvar ;
        dct:identifier   "3729468719998697358"^^<http://www.w3.org/2001/XMLSchema#long> ;
