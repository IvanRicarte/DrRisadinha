        dct:abstract     "A artrose, também chamada de osteoartrite, a depender do caso, é tratada com medicamentos, cirurgia, perda de peso, exercícios e uso de órteses." ;
        dct:contributor  doc:mcbg ;
        dct:creator      doc:gjmc ;
