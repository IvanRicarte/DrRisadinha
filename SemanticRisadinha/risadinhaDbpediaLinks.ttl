        a                sioc:Post ;
        dct:abstract     "Os sintomas da Chikungunya são tratados com medicação para febre e dores articulares. O paciente deve ficar em repouso absoluto e beber líquidos em quantidade." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:ccd ;
        dct:identifier   "3239780541083081960"^^<http://www.w3.org/2001/XMLSchema#long> ;
