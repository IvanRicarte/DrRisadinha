        a                sioc:Post ;
        dct:abstract     "A redução do ganho de peso excessivo durante a gravidez é realizado por meio de um planejamento alimentar e exercícios físicos adequados." ;
        dct:contributor  doc:nss , doc:mcbg ;
        dct:creator      doc:aaf ;
        dct:identifier   "8414831428255191488"^^<http://www.w3.org/2001/XMLSchema#long> ;
