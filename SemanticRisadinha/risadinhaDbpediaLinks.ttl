        a                sioc:Post ;
        dct:abstract     "Hipertensão pulmonar é o aumento da pressão nas artérias (vasos sanguíneos) que levam sangue do coração aos pulmões, gerando falta de ar, fraqueza e cansaço." ;
        dct:contributor  doc:fc , doc:mcbg ;
