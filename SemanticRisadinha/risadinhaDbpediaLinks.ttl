        a                sioc:Post ;
        dct:abstract     "O tratamento para o vício em jogos eletrônicos é baseado no controle do tempo de jogo pelos responsáveis e em terapia com um psicólogo." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:gnc ;
        dct:identifier   "8191748863556937944"^^<http://www.w3.org/2001/XMLSchema#long> ;
