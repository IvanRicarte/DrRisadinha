doc:8430977676388583850
        a                sioc:Post ;
        dct:abstract     "Trata-se a depressão pós-parto com psicoterapia, medicamentos antidepressivos e atividade física." ;
        dct:contributor  doc:nss , doc:mcbg ;
        dct:creator      doc:aaf ;
        dct:identifier   "8430977676388583850"^^<http://www.w3.org/2001/XMLSchema#long> ;
