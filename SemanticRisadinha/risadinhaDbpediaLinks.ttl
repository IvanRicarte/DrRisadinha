        a                sioc:Post ;
        dct:abstract     "O tratamento da distrofia muscular congênita é feito de acordo com os sintomas apresentados pela criança e requer a ajuda de uma equipe multiprofissional." ;
        dct:contributor  doc:fc , doc:mcbg ;
        dct:creator      doc:nss ;
        dct:identifier   "7337443630762524158"^^<http://www.w3.org/2001/XMLSchema#long> ;
